template/sparkling-app-template/android/.gradle/
template/sparkling-app-template/ios/build/
template/sparkling-app-template/ios/DerivedData/
template/sparkling-app-template/ios/Pods/
template/sparkling-app-template/coverage/
template/sparkling-app-template/vendor/bundle/
template/sparkling-app-template/.bundle/

tsconfig.tsbuildinfo

