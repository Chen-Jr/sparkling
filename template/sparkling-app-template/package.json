{
  "name": "sparkling-app-template",
  "version": "2.0.0-rc.0",
  "description": "Brownfield Lynx app based on Sparkling",
  "homepage": "",
  "repository": {
    "type": "git",
    "url": "https://github.com/tiktok/sparkling",
    "directory": "template/sparkling-app-template"
  },
  "type": "module",
  "scripts": {
    "build": "sparkling-cli build",
    "build:copy": "sparkling-cli build --copy",
    "autolink": "sparkling-cli autolink",
    "format": "prettier --write .",
    "test": "vitest run --passWithNoTests",
    "test:coverage": "vitest run --coverage --passWithNoTests",

    "run:android": "sparkling-cli run:android",
    "run:ios": "sparkling-cli run:ios"
  },
  "dependencies": {
    "@lynx-js/react": "^0.116.2",
    "sparkling-router": "rc"
  },
  "devDependencies": {
    "@lynx-js/qrcode-rsbuild-plugin": "^0.4.4",
    "@lynx-js/react-rsbuild-plugin": "^0.12.7",
    "@lynx-js/rspeedy": "^0.13.3",
    "@lynx-js/types": "^3.6.0",
    "@testing-library/jest-dom": "^6.6.3",
    "@types/react": "^18.3.20",
    "jsdom": "^26.1.0",
    "prettier": "^3.5.3",
    "typescript": "~5.8.3",
    "vitest": "^3.1.2",
    "@vitest/coverage-v8": "^3.1.2",
    "sparkling-cli": "rc"
  },
  "engines": {
    "node": "^22 || ^24"
  },
  "private": false
}
