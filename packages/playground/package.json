{
  "name": "sparkling-playground",
  "version": "2.0.0-rc.5",
  "type": "module",
  "description": "Multi-page Sparkling app demos with full features",
  "homepage": "",
  "repository": {
    "type": "git",
    "url": "https://github.com/tiktok/sparkling",
    "directory": "packages/playground"
