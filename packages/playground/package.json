    "@lynx-js/types": "^3.6.0",
    "@rsbuild/core": "1.7.2",
    "@testing-library/jest-dom": "^6.6.3",
    "@types/react": "^18.3.20",
    "jsdom": "^26.1.0",
    "prettier": "^3.5.3",
    "typescript": "^5.8.3",
    "vitest": "^3.1.2",
    "@vitest/coverage-v8": "^3.1.2"
